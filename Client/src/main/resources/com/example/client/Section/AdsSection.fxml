<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefWidth="570.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.example.client.Dashboard.AdsController">
   <HBox alignment="TOP_CENTER" minHeight="55.0" prefHeight="55.0" prefWidth="570.0" styleClass="top-title"
         stylesheets="@../../../../Style/adsSection.css">
      <padding>
         <Insets top="10.0"/>
      </padding>
      <ImageView fitHeight="30.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../image/home.png"/>
         </image>
      </ImageView>
      <Label alignment="CENTER" minHeight="30.0" prefHeight="21.0" prefWidth="58.0" text="Ads">
         <font>
            <Font name="System Bold" size="25.0"/>
         </font>
      </Label>
   </HBox>
   <TabPane prefWidth="570.0" tabClosingPolicy="UNAVAILABLE">
      <Tab text="Home">
         <VBox prefHeight="200.0" prefWidth="100.0">
            <HBox alignment="CENTER" minHeight="110.0" prefHeight="110.0" prefWidth="570.0" spacing="50.0">
               <VBox.margin>
                  <Insets/>
               </VBox.margin>
               <ComboBox fx:id="City" prefHeight="36.0" prefWidth="240.0" promptText="City" styleClass="box"
                         stylesheets="@../../../../Style/adsSection.css"/>
               <HBox fx:id="searchHbox" alignment="CENTER" maxHeight="35.0" prefHeight="35.0"
                     prefWidth="240.0" styleClass="box" stylesheets="@../../../../Style/adsSection.css">
                  <Button alignment="CENTER" contentDisplay="CENTER" minHeight="33.0" minWidth="33.0"
                          mnemonicParsing="false" onAction="#searchButton" prefHeight="34.0"
                          prefWidth="43.0" style="-fx-background-color: transparent;"
                          textAlignment="CENTER">
                     <graphic>
                        <ImageView fitHeight="27.0" fitWidth="22.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../image/sreach-2.png"/>
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <TextField fx:id="searchTextbox" prefHeight="34.0" prefWidth="195.0"
                             styleClass="search-btn" stylesheets="@../../../../Style/adsSection.css"/>
               </HBox>
            </HBox>
            <Accordion prefWidth="570.0">
               <panes>
                  <TitledPane animated="false" prefHeight="313.0" prefWidth="570.0"
                              stylesheets="@../../../../Style/adsSection.css" text="Filter">
                     <font>
                        <Font name="System Bold" size="14.0"/>
                     </font>
                     <VBox prefHeight="338.0" prefWidth="569.0">
                        <Label prefHeight="27.0" prefWidth="104.0" text="Categories:">
                           <font>
                              <Font name="System Bold" size="14.0"/>
                           </font>
                        </Label>
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="546.0">
                           <ComboBox fx:id="mainBranchCategories" minHeight="36.0"
                                     onAction="#MainBranchCategotiesFunction" prefHeight="36.0"
                                     prefWidth="380.0" styleClass="box"
                                     stylesheets="@../../../../Style/adsSection.css">
                              <HBox.margin>
                                 <Insets left="30.0"/>
                              </HBox.margin>
                           </ComboBox>
                        </HBox>
                        <HBox alignment="CENTER" layoutX="21.0" layoutY="42.0" prefHeight="100.0"
                              prefWidth="546.0">
                           <ComboBox fx:id="branchTwoCategories" minHeight="36.0" prefHeight="36.0"
                                     prefWidth="380.0" styleClass="box"
                                     stylesheets="@../../../../Style/adsSection.css">
                              <HBox.margin>
                                 <Insets left="30.0" top="15.0"/>
                              </HBox.margin>
                           </ComboBox>
                        </HBox>
                        <Label layoutX="21.0" layoutY="21.0" prefHeight="27.0" prefWidth="104.0"
                               text="Price Range :">
                           <font>
                              <Font name="System Bold" size="14.0"/>
                           </font>
                           <VBox.margin>
                              <Insets bottom="10.0" top="10.0"/>
                           </VBox.margin>
                        </Label>
                        <HBox alignment="CENTER" layoutX="21.0" layoutY="48.0" prefHeight="58.0"
                              prefWidth="508.0">
                           <Label prefHeight="26.0" prefWidth="42.0" text="from:">
                              <font>
                                 <Font name="Segoe UI" size="14.0"/>
                              </font>
                           </Label>
                           <TextField fx:id="priceFrom" minHeight="36.0" prefHeight="36.0"
                                      prefWidth="160.0" styleClass="box"
                                      stylesheets="@../../../../Style/adsSection.css">
                              <HBox.margin>
                                 <Insets right="20.0"/>
                              </HBox.margin>
                           </TextField>
                           <Label layoutX="180.0" layoutY="35.0" prefHeight="26.0" prefWidth="26.0"
                                  text="to:">
                              <font>
                                 <Font size="14.0"/>
                              </font>
                           </Label>
                           <TextField fx:id="priceTo" layoutX="201.0" layoutY="18.0" minHeight="36.0"
                                      prefHeight="36.0" prefWidth="160.0" styleClass="box"
                                      stylesheets="@../../../../Style/adsSection.css"/>
                        </HBox>
                        <HBox alignment="CENTER" minHeight="50.0" minWidth="60.0" prefHeight="50.0"
                              prefWidth="570.0" spacing="50.0">
                           <padding>
                              <Insets left="40.0"/>
                           </padding>
                           <VBox.margin>
                              <Insets top="30.0"/>
                           </VBox.margin>
                           <JFXButton minHeight="40.0" onAction="#applyButton" prefHeight="40.0"
                                      prefWidth="386.0" ripplerFill="WHITE" styleClass="apply-btn"
                                      stylesheets="@../../../../Style/adsSection.css" text="Apply">
                              <font>
                                 <Font name="System Bold" size="16.0"/>
                              </font>
                           </JFXButton>
                        </HBox>
                     </VBox>
                  </TitledPane>
               </panes>
               <cursor>
                  <Cursor fx:constant="HAND"/>
               </cursor>
            </Accordion>
            <Pagination fx:id="pagination" pageCount="10" prefWidth="570.0"/>
         </VBox>
      </Tab>
      <Tab text="Recommended">
         <VBox prefHeight="200.0" prefWidth="100.0">
            <HBox alignment="CENTER" minHeight="110.0" prefHeight="110.0" prefWidth="570.0" spacing="50.0">
               <ComboBox fx:id="City1" prefHeight="36.0" prefWidth="240.0" promptText="City"
                         styleClass="box" stylesheets="@../../../../Style/adsSection.css"/>
            </HBox>
            <Pagination fx:id="pagination1" pageCount="10" prefWidth="570.0"/>
         </VBox>
      </Tab>
   </TabPane>
</VBox>
